{
  "manifest_version": 2,
  "name": "Net-Manthan Extension",
  "version": "0.1",
  "permissions": ["downloads", "downloads.open", "nativeMessaging"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "applications": {
    "gecko": {
      "id": "netmanthan@example.com"
    }
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "description": "Intercept and manage downloads through Net-Manthan."
}
