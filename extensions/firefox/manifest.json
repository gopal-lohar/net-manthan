{
  "manifest_version": 2,
  "name": "Net Manthan",
  "version": "1.0",
  "description": "Download Manager Extension",
  "permissions": ["downloads", "nativeMessaging", "storage"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "net-manthan@example.com"
    }
  }
}
